{
  "name": "embers-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit --project tsconfig.app.json",
    "format:code": "prettier --write '**/*.{js,ts,tsx,scss}' --ignore-path ../../.prettierignore",
    "format:scss": "stylelint --fix '**/*.scss'",
    "ci:check:code": "prettier --check '**/*.{js,ts,tsx,scss}' --ignore-path ../../.prettierignore",
    "ci:check:scss": "stylelint '**/*.scss'"
  },
  "dependencies": {
    "@noble/curves": "^1.9.4",
    "@scure/base": "^1.2.6",
    "@tanstack/react-query": "^5.83.0",
    "blakejs": "^1.2.1",
    "classnames": "^2.5.1",
    "js-sha3": "^0.9.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.0",
    "zod": "^4.0.5"
  }
}
