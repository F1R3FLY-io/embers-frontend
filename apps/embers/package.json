{
  "name": "@f1r3fly-io/embers-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --fix .",
    "preview": "vite preview",
    "typecheck": "tsc -b --noEmit",
    "format:code": "prettier --write '**/*.{js,jsx,ts,tsx,html,svg,json,css,scss}'",
    "format:css": "stylelint --fix '**/*.{css,scss}'",
    "ci:lint": "eslint .",
    "ci:check:code": "prettier --check '**/*.{js,jsx,ts,tsx,html,svg,json,css,scss}'",
    "ci:check:css": "stylelint '**/*.{css,scss}'"
  },
  "dependencies": {
    "@f1r3fly-io/embers-client-sdk": "workspace:*",
    "@scure/base": "^1.2.6",
    "@tanstack/react-query": "^5.85.3",
    "classnames": "^2.5.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.8.1"
  },
  "devDependencies": {
    "@svgr/plugin-svgo": "^8.1.0",
    "vite-plugin-svgr": "^4.3.0"
  }
}
