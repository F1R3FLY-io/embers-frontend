{
  "name": "@f1r3fly-io/embers-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm --filter @f1r3fly-io/embers-client-sdk build && tsc -b && vite build",
    "lint": "eslint --fix .",
    "preview": "vite preview",
    "typecheck": "tsc -b --noEmit",
    "format:code": "prettier --write '**/*.{js,jsx,cjs,ts,tsx,mts,html,svg,json,css,scss}'",
    "format:css": "stylelint --fix '**/*.{css,scss}'",
    "ci:lint": "eslint .",
    "ci:check:code": "prettier --check '**/*.{js,jsx,cjs,ts,tsx,mts,html,svg,json,css,scss}'",
    "ci:check:css": "stylelint '**/*.{css,scss}'",
    "sync-i18n": "node --loader ts-node/esm sync-i18n.mts"
  },
  "dependencies": {
    "@codemirror/commands": "^6.10.1",
    "@codemirror/state": "^6.5.2",
    "@f1r3fly-io/embers-client-sdk": "workspace:*",
    "@f1r3fly-io/lightning-bug": "0.7.7",
    "@f1r3fly-io/tree-sitter-rholang-js-with-comments": "1.1.9",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@scure/base": "^2.0.0",
    "@szhsin/react-menu": "^4.5.1",
    "@tanstack/react-query": "^5.90.12",
    "@xyflow/react": "^12.10.0",
    "classnames": "^2.5.1",
    "dagre": "^0.8.5",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.3.8",
    "react-i18next": "^16.5.0",
    "react-modal": "^3.16.3",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/dagre": "^0.7.53",
    "@types/node": "^25.0.3",
    "@types/react-modal": "^3.16.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite-plugin-static-copy": "^3.1.4",
    "vite-plugin-svgr": "^4.5.0"
  }
}
