{
  "name": "@f1r3fly-io/embers-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --fix .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format:code": "prettier --write '**/*.{js,jsx,ts,tsx,html,svg,json,css,scss}'",
    "format:css": "stylelint --fix '**/*.{css,scss}'",
    "ci:lint": "eslint .",
    "ci:check:code": "prettier --check '**/*.{js,jsx,ts,tsx,html,svg,json,css,scss}'",
    "ci:check:css": "stylelint '**/*.{css,scss}'"
  },
  "dependencies": {
    "@codemirror/commands": "^6.8.1",
    "@codemirror/state": "^6.5.2",
    "@f1r3fly-io/embers-client-sdk": "workspace:*",
    "@scure/base": "^2.0.0",
    "@szhsin/react-menu": "^4.4.1",
    "@tanstack/react-query": "^5.85.5",
    "@xyflow/react": "^12.8.4",
    "classnames": "^2.5.1",
    "font-awesome": "^4.7.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-modal": "^3.16.3",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/react-modal": "^3.16.3",
    "vite-plugin-svgr": "^4.5.0"
  },
  "optionalDependencies": {
    "@f1r3fly-io/lightning-bug": "0.3.0"
  }
}
