{
  "name": "@f1r3fly-io/embers-client-sdk",
  "version": "0.0.37",
  "main": "dist/embers-client-sdk.cjs.js",
  "module": "dist/embers-client-sdk.es.js",
  "browser": "dist/embers-client-sdk.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/embers-client-sdk.es.js",
    "require": "./dist/embers-client-sdk.cjs.js"
  },
  "description": "A set of functions for working with wallets and contracts on the blockchain",
  "author": "Denys Bushualik <denys.b@devbrother.com>",
  "license": "UNLICENSED",
  "type": "module",
  "keywords": [
    "F1r3Sky",
    "Wallet",
    "Blockchain",
    "SmartContracts"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "config": {
    "doc_port": 3000
  },
  "scripts": {
    "build": "vite build && tsc && cp src/declarations/*.d.ts dist/",
    "test": "node test-with-mocks.js",
    "coverage": "node test-with-mocks.js --coverage",
    "lint": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "gen-docs": "typedoc",
    "docs": "pnpm run gen-docs && serve -l $npm_package_config_doc_port docs",
    "generate-mocks": "counterfact ./schema.json mocks --generate",
    "generate-api-client": "docker run -v $PWD:/local openapitools/openapi-generator-cli generate -i /local/schema.json -g typescript-fetch -o /local/src/api-client -c /local/openapitools.json",
    "format:code": "prettier --write '**/*.{js,jsx,ts,tsx,html,svg,json}'",
    "ci:lint": "eslint .",
    "ci:check:code": "prettier --check '**/*.{js,jsx,ts,tsx,html,svg,json}'"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "counterfact": "^1.4.2",
    "eslint": "^9.34.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "serve": "^14.2.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "typedoc": "^0.28.12",
    "vite": "^7.1.4"
  },
  "dependencies": {
    "@noble/curves": "^2.0.0",
    "@scure/base": "^2.0.0",
    "blakejs": "^1.2.1",
    "js-sha3": "^0.9.3",
    "zod": "^4.1.5"
  }
}
