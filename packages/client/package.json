{
  "name": "@f1r3fly-io/embers-client-sdk",
  "version": "0.0.59",
  "main": "dist/embers-client-sdk.cjs.js",
  "module": "dist/embers-client-sdk.es.js",
  "browser": "dist/embers-client-sdk.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/embers-client-sdk.es.js",
    "require": "./dist/embers-client-sdk.cjs.js"
  },
  "description": "A set of functions for working with wallets and contracts on the blockchain",
  "author": "Denys Bushualik <denys.b@devbrother.com>",
  "license": "UNLICENSED",
  "type": "module",
  "keywords": [
    "F1r3Sky",
    "Wallet",
    "Blockchain",
    "SmartContracts"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "config": {
    "doc_port": 3000
  },
  "scripts": {
    "build": "vite build",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint --fix .",
    "typecheck": "tsc -b --noEmit",
    "gen-docs": "typedoc",
    "docs": "pnpm run gen-docs && serve -l $npm_package_config_doc_port docs",
    "generate-api-client": "docker run -v $PWD:/local openapitools/openapi-generator-cli generate -i /local/schema.json -g typescript-fetch -o /local/src/api-client -c /local/openapitools.json",
    "compile-proto": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts_proto --ts_out=src/generated --ts_opt=esModuleInterop=true,forceLong=bigint -I=./protobuf -I=./protobuf/protobuf_external ./protobuf/RhoTypes.proto",
    "format:code": "prettier --write '**/*.{js,jsx,ts,tsx,html,svg,json}'",
    "ci:lint": "eslint .",
    "ci:check:code": "prettier --check '**/*.{js,jsx,ts,tsx,html,svg,json}'"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "eslint-plugin-unused-imports": "^4.2.0",
    "express": "^5.1.0",
    "serve": "^14.2.5",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.7.7",
    "typedoc": "^0.28.14",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.9.0",
    "@f1r3fly-io/graphl-parser": "^0.0.21",
    "@noble/curves": "^2.0.1",
    "@scure/base": "^2.0.0",
    "blakejs": "^1.2.1",
    "js-crc": "^0.3.1",
    "js-sha3": "^0.9.3",
    "z32": "^1.1.0",
    "zod": "^4.1.12"
  }
}
