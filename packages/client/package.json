{
  "name": "@f1r3fly-io/embers-client-sdk",
  "version": "0.0.16",
  "main": "dist/embers-client-sdk.cjs.js",
  "module": "dist/embers-client-sdk.es.js",
  "browser": "dist/embers-client-sdk.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/embers-client-sdk.es.js",
    "require": "./dist/embers-client-sdk.cjs.js"
  },
  "description": "A set of functions for working with wallets and contracts on the blockchain",
  "author": "Denys Bushualik <denys.b@devbrother.com>",
  "license": "UNLICENSED",
  "type": "module",
  "keywords": [
    "F1r3Sky",
    "Wallet",
    "Blockchain",
    "SmartContracts"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "config": {
    "doc_port": 3000
  },
  "scripts": {
    "build": "vite build && tsc -p tsconfig.lib.json --emitDeclarationOnly",
    "test": "node test-with-mocks.js",
    "coverage": "node test-with-mocks.js --coverage",
    "lint": "eslint --fix .",
    "typecheck": "tsc -b --noEmit",
    "gen-docs": "typedoc",
    "docs": "pnpm run gen-docs && serve -l $npm_package_config_doc_port docs",
    "generate-api-client": "docker run -v $PWD:/local openapitools/openapi-generator-cli generate -i /local/schema.json -g typescript-fetch -o /local/src/api-client -c /local/openapitools.json",
    "format:code": "prettier --write '**/*.{js,jsx,ts,tsx,html,svg,json}'",
    "ci:lint": "eslint .",
    "ci:check:code": "prettier --check '**/*.{js,jsx,ts,tsx,html,svg,json}'"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "counterfact": "^1.4.1",
    "express": "^5.1.0",
    "serve": "^14.2.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.9"
  },
  "dependencies": {
    "@noble/curves": "^1.9.6",
    "@scure/base": "^1.2.6",
    "blakejs": "^1.2.1",
    "js-sha3": "^0.9.3",
    "zod": "^4.0.14"
  }
}
