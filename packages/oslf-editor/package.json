{
  "name": "@f1r3fly-io/oslf-editor",
  "version": "0.6.0",
  "description": "Operational Semantics in Logical Form editor",
  "keywords": [
    "OSLF",
    "editor",
    "RhoLang",
    "F1r3fly"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc --project tsconfig.lib.json && vite build",
    "test": "jest",
    "lint": "eslint --fix .",
    "typecheck": "tsc -b --noEmit",
    "format:code": "prettier --write '**/*.{js,jsx,cjs,ts,tsx,mts,html,svg,json,css,scss}'",
    "format:css": "stylelint --allow-empty-input --fix '**/*.{css,scss}'",
    "ci:lint": "eslint .",
    "ci:check:code": "prettier --check '**/*.{js,jsx,cjs,ts,tsx,mts,html,svg,json,css,scss}'",
    "ci:check:css": "stylelint '**/*.{css,scss}'"
  },
  "dependencies": {
    "@blockly/plugin-cross-tab-copy-paste": "^8.0.7",
    "blockly": "^12.3.1"
  }
}
