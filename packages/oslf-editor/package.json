{
  "name": "@f1r3fly-io/oslf-editor",
  "version": "0.6.0",
  "description": "Operational Semantics in Logical Form editor with React components",
  "keywords": [
    "OSLF",
    "editor",
    "RhoLang",
    "F1r3fly"
  ],
  "homepage": "https://github.com/F1R3FLY-io/OSLF-editor#readme",
  "bugs": {
    "url": "https://github.com/F1R3FLY-io/OSLF-editor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/F1R3FLY-io/OSLF-editor.git"
  },
  "license": "Apache-2.0",
  "author": "Denys Bushuliak <denys.b@devbrother.com>",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles": "./dist/styles/blockly-overrides.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.lib.json && vite build",
    "dev": "vite build --watch",
    "test": "../../../OSLF-editor/editor/node_modules/.bin/vitest run",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc -b tsconfig.lib.json --noEmit",
    "format:code": "prettier --write '**/*.{js,jsx,ts,tsx,html,svg,json}'",
    "ci:lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "ci:check:code": "prettier --check '**/*.{js,jsx,ts,tsx,html,svg,json}'",
    "clean": "rm -rf dist ../../../OSLF-editor/editor/node_modules"
  },
  "dependencies": {
    "@blockly/plugin-cross-tab-copy-paste": "^8.0.2",
    "blockly": "^12.3.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "prettier": "^3.8.1",
    "react": "^19.0.0",
    "sass": "^1.85.0",
    "typescript": "^5.9.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16"
  }
}
